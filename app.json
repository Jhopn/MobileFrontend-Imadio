{
  "expo": {
    "name": "Imadio",
    "slug": "imadio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "backgroundColor": "#eceaff",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/jhopn.png",
        "backgroundColor": "#eceaff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/jhopn.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#eceaff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para converter em Ã¡udio."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}